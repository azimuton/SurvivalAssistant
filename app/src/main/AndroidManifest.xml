<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jkdajac.survivalassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_survival"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_survival_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SurvivalAssistant">
        <activity
            android:name=".presentation.dictaphone.DictaphoneActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.stopwatch.StopwatchActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.TimerActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.NoteActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.SledActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.CompassActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>